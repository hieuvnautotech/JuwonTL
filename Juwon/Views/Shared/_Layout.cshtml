                            <a href="#">
                                <img src="/Img/Menu/VN.png" class="mr-3 mt-3" style="width: 40px; height: 40px" id="i_VN">
                            </a>
                        </li>
                        <li class="nav-item dropdown no-arrow">
                            @using (Html.BeginForm("ChangeCulture", "Base", FormMethod.Post, new { @class = "login100-form" }))
                            {
                                <div class="wrap-input100">@Html.Hidden("returnUrl", Request.RawUrl) @Html.DropDownList("ddlCulture", new SelectList(new[] { new { value = "en", text = Library.Resource.English }, new { value = "vi", text = Library.Resource.Vietnamese }, new { value = "ko", text = Library.Resource.Korean } }, "value", "text", Library.Helper.SessionHelper.GetCultureSession()), new { @class = "nav-link dropdown-toggle", onchange = "this.form.submit();", @style = "border-radius: 5px; border: none; color: white;background-color: black" })</div>}
                        </li>
                        <div class="topbar-divider d-none d-sm-block"></div>
                        <!-- Nav Item - User Information -->
                        <li class="nav-item dropdown no-arrow">
                            <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                                <span id="username" class="mr-2 d-none d-lg-inline text-gray-600 small" style="color:white !important"></span>
                                <img class="img-profile rounded-circle" src="~/Img/header/undraw_profile.svg">
                            </a>
                            <!-- Dropdown - User Information -->
                            <div class="dropdown-menu dropdown-menu-right shadow animated--grow-in" aria-labelledby="userDropdown">
