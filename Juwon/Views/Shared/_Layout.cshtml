                    </button>
                </div>
                <div class="modal-body">@Html.Raw(Library.Resource.MSG_ConfirmToLogout)</div>
                <div class="modal-footer">
                    <a class="btn btn-secondary" href="#" data-dismiss="modal">@Html.Raw(Library.Resource.Btn_Cancel)</a>
                    <a class="btn btn-primary" href="/Login/Logout">@Html.Raw(Library.Resource.Btn_Logout)</a>
