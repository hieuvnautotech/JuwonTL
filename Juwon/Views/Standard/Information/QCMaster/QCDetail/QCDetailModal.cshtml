<div id="qcDetailListModal" class="modal fade" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel" aria-hidden="true">
    @* QCDetail Grid *@
    <div class="modal-dialog modal-dialog-centered modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Select QC Detail</h5>
                <button class="close iBtnSearch" type="button" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">×</span>
                </button>
            </div>
            <div class="modal-body container-fluid">
                <div class="box-body my-2 bg-white">

                    <div class="row p-0 m-0">
                        <table id="qcDetailGrid"></table>
                    </div>

                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-success button-sm Permission_createQCMaster" data-dismiss="modal" id="createQCDetailList"><i class="fa fa-save"></i>&nbsp;@Html.Raw(Library.Resource.Btn_Save)</button>
            </div>
        </div>
    </div>
</div>
